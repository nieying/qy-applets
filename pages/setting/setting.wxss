.page-setting {
  height: 100%;
  background: #eaedf2;
}

.warp,
.list {
  margin-top: 32rpx;
}

.list .item {
  display: flex;
  height: 112rpx;
  background: rgba(245, 247, 249, 1);
  border-radius: 20rpx;
  padding: 30rpx 30rpx;
  align-items: center;
  justify-content: space-between;
  margin: 20rpx 0;
}

.list .item .left {
  margin-right: 24rpx;
}


.name {
  font-size: 32rpx;
  font-weight: 600;
  color: rgba(40, 45, 75, 1);
  line-height: 48rpx;
}

.desc {
  font-size: 24rpx;
  font-weight: 400;
  color: rgba(144, 164, 174, 1);
}