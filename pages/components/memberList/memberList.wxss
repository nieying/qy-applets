.list .type1 {
  background: white;
  padding: 0 30rpx;
}

.list .type1 text {
  background: rgba(176, 190, 197, 1);
  border-radius: 20rpx;
  font-size: 20rpx;
  font-weight: 600;
  color: white;
  border-bottom: 1rpx solid #eaedf2;
  margin: 24rpx 0;
  display: inline-block;
  padding: 8rpx 16rpx;
  /* display: flex;
  justify-content: center;
  align-items: center; */
}

.list .item {
  height: 136rpx;
  padding: 0 30rpx;
  display: flex;
  align-items: center;
  justify-content: center;
  background: rgba(255, 255, 255, 1);
  box-shadow: 0px 1px 0px 0px rgba(234, 237, 242, 1), 0px -1px 0px 0px rgba(234, 237, 242, 1);
}

.list .item .left {
  flex: 1;
  display: flex;
}

.list .item image {
  width: 88rpx;
  height: 88rpx;
  background: rgba(176, 190, 197, 1);
  border-radius: 50%;
  margin-right: 24rpx;
}

.list .item .info .name {
  font-size: 32rpx;
  font-weight: 600;
  color: rgba(40, 45, 75, 1);
}

.list .item .info .desc, .list .item .right text {
  font-size: 24rpx;
  font-weight: 400;
  color: rgba(144, 164, 174, 1);
}

.right .pass {
  display: flex;
}

.btn {
  background: rgba(0, 200, 83, 1);
  border-radius: 8rpx;
  color: white;
  padding: 10rpx 24rpx;
  margin: 0 10rpx;
}
.red {
  background: #F44336;
}
