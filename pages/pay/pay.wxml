<view class="page-vip">
  <view class="header" style='padding-top: {{height + 10}}px'>
    <icon type="clear" size="30" color="black" bindtap="goBack" />
  </view>
  <scroll-view scroll-y="true" style="height: {{warpHeight}}px" class="content">
    <view class="padding-box">
      <view class='title'>VIP充值</view>
      <view class='list'>
        <view class="item {{item.checked ?'active':''}}" wx:for="{{moneyList}}" wx:for-item="item" wx:key="index" data-index="{{index}}" bindtap='selectMoney'>
          {{item.value}} 元
        </view>
      </view>
    </view>
  </scroll-view>
  <view class="bottom" bindtap="payMoney">
    <text>充值</text>
  </view>
</view>