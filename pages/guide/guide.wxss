page {
  height: 100%;
  position: relative;
}

.page-guide {
  height: 100%;
  background: #eaedf2;
}

.guide-warp {
  position: relative;
  height: 100%;
  background: white;
  padding: 248rpx 40rpx 40rpx 40rpx;
}

.logo image {
  width: 360rpx;
  height: 360rpx;
}

.icon-success {
  width: 33rpx;
  height: 24rpx;
}

.name {
  display: flex;
  justify-content: space-between;
  align-items: center;
  margin-top: 52rpx;
  margin-left: 30rpx;
  line-height:44rpx;
}

.name text {
  font-size: 28rpx;
  font-weight: 400;
  color: rgba(144, 164, 174, 1);
  line-height: 44rpx;
}

.name image {
  width: 64rpx;
  height: 64rpx;
}

.guide-btn {
  position: absolute;
  bottom: 300rpx;
  text-align: center;
  margin-left: -184rpx;
  left: 50%; 
}

button {
  /* margin-top: 100rpx; */
  font-weight: 100;
  line-height: 80rpx;
  border: none;
  width: 368rpx;
  height: 80rpx;
  background: rgba(0, 200, 83, 1);
  font-size: 32rpx;
  border-radius: 8rpx;
  color: white;
  font-family: PingFangSC-Medium, PingFang SC;
  border: none;
}

button::after {
  border: none !important;
  box-sizing: none !important;
}

.cancel-btn {
  display: flex;
  justify-content: center;
  align-items: center;
  height: 80rpx;
  background-color: white;
  color: rgba(144, 164, 174, 1);
  font-size: 32rpx;
  border-radius: 8rpx;
  font-family: PingFangSC-Medium, PingFang SC;
  margin-top: 20rpx;
}

