page {
  height: 100%;
  position: relative;
}

.page-guide {
  height: 100%;
  background: #eaedf2;
}

.guide-warp {
  height: 100%;
  background: white;
  padding: 48rpx 40rpx;
}

.logo image {
  width: 360rpx;
  height: 360rpx;
}

.name {
  display: flex;
  justify-content: space-between;
  align-items: center;
  margin-top: 64rpx;
  margin-left: 30rpx;
}

.name text {
  font-size: 28rpx;
  font-weight: 400;
  color: rgba(144, 164, 174, 1);
  line-height: 44rpx;
}

.guide-btn {
  position: absolute;
  width: 100%;
  bottom: 312rpx;
  text-align: center;
}

button {
  /* margin-top: 100rpx; */
  font-weight: 100;
  line-height: 76rpx;
  border: none;
  width: 368rpx;
  height: 80rpx;
  background: rgba(0, 200, 83, 1);
  border-radius: 8rpx;
  color: white;
  font-family: PingFangSC-Medium, PingFang SC;
}

.guide-bottom {
  width: 100%;
  position: absolute;
  bottom: 0;
  color: #90a4ae;
  text-align: center;
}

.qianyan {
  font-size: 32rpx;
  font-family: PingFangSC-Semibold, PingFang SC;
  font-weight: 600;
  color: rgba(144, 164, 174, 1);
  line-height: 48rpx;
}

.copy-right {
  font-size: 24rpx;
  font-family: FFDINPro-MediumItalic, FFDINPro;
  font-weight: normal;
  color: rgba(144, 164, 174, 1);
  line-height: 36rpx;
}
