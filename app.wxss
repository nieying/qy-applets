page {
  height: 100%;
  position: relative;
  z-index: 9999998;
  color: #282d4b;
  /* font-family: PingFangSC-Regular, PingFang SC; */
  /* overflow: hidden; */
  /* height: clac(100% - 88rpx); */
}

::-webkit-scrollbar {
  width: 0;
  height: 0;
  color: transparent;
}

.container {
  height: clac(100% - 88rpx);
  display: flex;
  flex-direction: column;
  box-sizing: border-box;
  background: #eaedf2;
  font-size: 26rpx;
  width: 100%;
  overflow-x: hidden;
}

.header {
  display: flex;
  align-items: center;
  padding-left: 30rpx;
  padding-bottom: 30rpx;
  /* padding: 10rpx 0 0rpx 30rpx; */
}

.header .title {
  font-size: 44rpx;
  font-weight: 600;
}

.title .line {
  width: 6rpx;
  height: 40rpx;
  background: #ffd100;
  margin-right: 16rpx;
  border-radius: 20rpx;
}

.header .money {
  display: flex;
  align-items: center;
}

.header .money image {
  width: 64rpx;
  height: 64rpx;
  margin-right: 15rpx;
}

.header .money {
  font-size: 28rpx;
  font-weight: 800;
  color: rgba(40, 45, 75, 1);
  line-height: 44rpx;
}

.go-back {
  width: 64rpx;
  height: 64rpx;
}

.title {
  display: flex;
  align-items: center;
  font-weight: 600;
  font-size: 38rpx;
}

.content {
  background: white;
  border-radius: 40rpx 40rpx 0 0;
  padding-top: 48rpx;
  overflow-y: auto;
}

.padding-box {
  padding: 0 40rpx;
}

.bottom {
  position: fixed;
  bottom: 0;
  left: 0;
  width: 100%;
  height: 176rpx;
  background-color: #282d4b;
  z-index: 500;
  border-radius: 40rpx 40rpx 0 0;
  display: flex;
  justify-content: center;
}

.bottom text {
  padding-top: 50rpx;
  color: #ffd100;
  font-size: 36rpx;
  font-family: PingFangSC-Semibold, PingFang SC;
  font-weight: 600;
  line-height: 56rpx;
}

.btn {
  font-size: 32rpx;
  padding: 8rpx 24rpx;
  border-radius: 8rpx;
}

.btn-primary {
  color: #282d4b;
  background: #ffd100;
}

.btn-black {
  color: #ffd100;
  background: #282d4b;
}

.btn-primary:hover {
  background: #fbc02e;
}

.btn-default {
  color: white;
  background: #282d4b;
}

.btn-default:hover {
  background: #1a1d30;
}

.btn-green {
  color: white;
  background: rgba(0, 200, 83, 1);
}

.btn-warn {
  color: white;
  background: #f44336;
}

/* .btn-warn:hover {
  background: #1a1d30;
} */

.warp {
  background: rgba(245, 247, 249, 1);
  border-radius: 20rpx;
  display: flex;
  flex-direction: column;
  justify-content: center;
  align-items: center;
  padding: 64rpx 0;
}


.warp .result {
  font-size: 38rpx;
  font-weight: 600;
  color: rgba(40, 45, 75, 1);
  margin: 16rpx 0 8rpx 0;
}

.warp .tips {
  font-size: 28rpx;
  color: rgba(144, 164, 174, 1);
  line-height: 48rpx;
  text-align: center;
  font-weight: 300;
}

.warp .kefu {
  height: 64rpx;
  background: rgba(40, 45, 75, 1);
  border-radius: 8rpx;
  font-size: 28rpx;
  font-weight: 500;
  color: rgba(255, 255, 255, 1);
  padding: 0 24rpx;
  display: flex;
  align-items: center;
  margin: 64rpx 0 32rpx 0;
}

.warp .copy {
  color: #90a4ae;
  font-size: 24rpx;
}

.warp image {
  width: 64rpx;
  height: 64rpx;
}

.no-datas {
  color: #90a4ae;
  font-size: 28rpx;
  display: flex;
  flex-direction: column;
  justify-content: center;
  align-items: center;
  margin: 30rpx 0;
  background: transparent;
  text-align: center;
}

.mask {
  position: fixed;
  left: 0;
  right: 0;
  top: 0;
  bottom: 0;
  display: flex;
  justify-content: center;
  align-items: center;
  background-color: rgba(0, 0, 0, 0.4);
  z-index: 9999;
}

.modal-content {
  display: flex;
  flex-direction: column;
  width: 90%;
  background-color: #fff;
  border-radius: 20rpx;
}

.modal-content textarea {
  margin-top: 50rpx;
  width: 90%;
  height: 100rpx;
  border-radius: 20rpx;
  padding: 35rpx 40rpx;
  border: 4rpx solid rgba(234, 237, 242, 1);
}

.modal-btn-wrapper {
  display: flex;
  flex-direction: row;
  height: 100rpx;
  line-height: 100rpx;
  border-top: 2rpx solid rgba(7, 17, 27, 0.1);
}

.cancel-btn,
.confirm-btn {
  flex: 1;
  height: 112rpx;
  line-height: 112rpx;
  text-align: center;
  font-size: 36rpx;
  color: #ffd100;
  background: #282d4b;
  border-radius: 0 0 20rpx 20rpx;
}

.main-content {
  flex: 1;
  width: 90%;
  height: 100%;
  overflow-y: hidden;
  padding: 48rpx;
}

.modal-title {
  font-size: 38rpx;
  font-weight: 600;
  color: rgba(40, 45, 75, 1);
  text-align: center;
}

.hover-grey {
  box-shadow: 0rpx 0rpx 8rpx #ccc inset;
}

.hover-white {
  box-shadow: 0rpx 0rpx 8rpx white inset;
}

.hover-black {
  box-shadow: 0rpx 0rpx 8rpx black inset;
}

.hover-red {
  box-shadow: 0rpx 0rpx 8rpx black inset;
}

.guide-bottom {
  width: 100%;
  position: absolute;
  bottom: 0;
  color: #90a4ae;
  text-align: center;
}

.guide-bottom .qianyan view {
  display: inline-block;
  width: 4rpx;
  background: rgba(144, 164, 174, 1);
  height: 28rpx;
  margin: 0 10rpx;
  opacity: 0.5;
}

.qianyan {
  font-size: 32rpx;
  font-family: PingFangSC-Semibold, PingFang SC;
  font-weight: 600;
  color: rgba(144, 164, 174, 1);
  line-height: 48rpx;
  display: flex;
  align-items: center;
  justify-content: center;
}

.copy-right {
  font-size: 24rpx;
  font-family: FFDINPro-MediumItalic, FFDINPro;
  font-weight: normal;
  color: rgba(144, 164, 174, 1);
  line-height: 36rpx;
  font-style: italic;
}